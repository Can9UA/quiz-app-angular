extends _modules/layout
block body
  main(ng-cloak).main
    .container
      section
        h1 Turtle facts Quiz
        h2 Learn about turtles below
        div(ng-controller="ListCtrl as list")
          .row
            .col-sm-6(ng-repeat="item in list.data")
              .well.well-sm
                .row
                  .col-md-6
                    img(ng-src="{{item.imageUrl}}" alt="")
              | {{item.type}}